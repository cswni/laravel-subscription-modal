{
    "name": "laravel-subscription-modal/subscription-modal",
    "description": "Paquete para mostrar un modal de suscripci√≥n en la parte inferior derecha de aplicaciones Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Andres Perez Ubeda",
            "email": "descarlos2013@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.45.1|^12.0",
                        "livewire/livewire": "^2.12"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.14|^10.0",
        "phpunit/phpunit": "^10.5.48"
    },
    "autoload": {
        "psr-4": {
            "LaravelSubscriptionModal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSubscriptionModal\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSubscriptionModal\\LaravelSubscriptionModalServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
